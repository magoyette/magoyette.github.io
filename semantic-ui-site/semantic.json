{
    "base": "semantic/",
    "paths": {
        "source": {
            "config": "src/theme.config",
            "definitions": "src/definitions/",
            "site": "src/site/",
            "themes": "src/themes/"
        },
        "output": {
            "packaged": "../resources/public/styles/",
            "uncompressed": "dist/components/",
            "compressed": "dist/components/",
            "themes": "../resources/public/themes/"
        },
        "clean": "dist/"
    },
    "permission": false,
    "rtl": false,
    "components": [
        "header",
        "icon",
        "label",
        "segment",
        "menu",
        "container"
    ],
    "autoInstall": true,
    "version": "2.2.2"
}
